{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(privacy|terms|data-deletion)",
      "destination": "/$1.html"
    },
    {
      "source": "/dashboard(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!\\.)(?!api\\/).)*",
      "destination": "/index.html"
    }
  ]
}


